@page
@model EVCharging.Pages.Auth.RegisterModel
@{
}

<h2>Register</h2>
<form method="post">
    <div asp-validation-summary="All"></div>
    <div><label>Full name</label><input asp-for="Input.FullName" /></div>
    <div><label>Email</label><input asp-for="Input.Email" /></div>
    <div><label>Password</label><input asp-for="Input.Password" type="password" /></div>
    <button type="submit">Create account</button>
</form>
<p class="text-success">@Model.Info</p>
